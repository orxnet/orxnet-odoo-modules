<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="subnet_list_view_form" model="ir.ui.view">
            <field name="name">Subnet Form</field>
            <field name="model">orxnet.subnet</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name" string="Name"/>
                        <field name="ip_network_string" string="Subnet"/>
                        <field name="ip_address_count" string="Number of Ip's" class="oe_readonly"/>
                    </group>
                </form>
            </field>
        </record>
        <record id="subnet_list_view_tree" model="ir.ui.view">
            <field name="name">Subnet List</field>
            <field name="model">orxnet.subnet</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" string="Name"/>
                    <field name="ip_network_string" string="Subnet"/>
                    <field name="ip_address_count" string="Number of Ip's"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>


